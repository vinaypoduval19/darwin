{
  "name": "ml-platform",
  "version": "1.0.58-SNAPSHOT",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "start:dev": "NODE_ENV=development nodemon",
    "start:prod": "NODE_ENV=production node server.js",
    "build": "tsc",
    "build:clean": "yarn clean && tsc",
    "build:watch": "tsc --watch",
    "clean": "find . -name '*.js' -not -path './node_modules/*' -not -path './scripts/*' -delete && find . -name '*.js.map' -not -path './node_modules/*' -delete",
    "dev": "tsc && nodemon",
    "dev:no-compile": "nodemon server.js",
    "lint": "yarn tsfiles | xargs tslint",
    "lint:diff": "git diff --cached --name-only | grep '.ts$' | xargs tslint",
    "lint:diff:fix": "git diff --cached --name-only | grep '.ts$' | xargs tslint --fix",
    "lint:fix": "yarn tsfiles | xargs tslint --fix",
    "prettier": "scripts/prettier.sh",
    "tsfiles": "git ls-files | grep '.ts$'"
  },
  "dependencies": {
    "@graphql-tools/load-files": "^6.3.2",
    "@types/moment-timezone": "^0.5.0",
    "apollo-server-express": "^3.1.2",
    "apollo-tracing": "^0.15.0",
    "axios": "^1.5.1",
    "body-parser": "^1.17.2",
    "colors": "^1.4.0",
    "cookie": "^0.5.0",
    "cors": "^2.8.4",
    "crypto-js": "^4.1.1",
    "dataloader": "^2.0.0",
    "date-fns": "2.16.1",
    "dateformat": "4.6.3",
    "dotenv": "^16.0.3",
    "express": "^4.15.3",
    "graphql": "^16.9.0",
    "graphql-tools": "^9.0.1",
    "moment-timezone": "^0.5.13",
    "query-string": "^7.1.3",
    "ramda": "^0.27.0",
    "url-pattern": "1.0.3",
    "winston": "^2.4.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.50.0",
    "@opentelemetry/exporter-metrics-otlp-proto": "^0.54.0",
    "@opentelemetry/exporter-trace-otlp-proto": "^0.54.0",
    "@opentelemetry/instrumentation-graphql": "^0.43.0",
    "@opentelemetry/sdk-metrics": "^1.27.0",
    "@opentelemetry/sdk-node": "^0.54.0",
    "@opentelemetry/sdk-trace-node": "^1.27.0"
  },
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/express": "^4.0.36",
    "@types/faker": "^5.5.8",
    "@types/mocha": "^9.0.0",
    "@types/ramda": "^0.27.44",
    "@types/sinon": "^10.0.2",
    "@types/node": "^18.0.0",
    "chai": "^4.1.1",
    "faker": "^4.1.0",
    "mocha": "^10.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.0.5",
    "sinon": "^11.1.2",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.2",
    "typescript": "^4.4.2"
  },
  "files": [
    "index.js",
    "../../modules"
  ]
}