<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://maven.apache.org/POM/4.0.0" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>com.dream11.ml_platform_mf</groupId>
  <artifactId>ml-platform-mf</artifactId>
  <packaging>pom</packaging>
  <version>1.0.0</version>
  <properties>
    <sonar.sources>./src</sonar.sources>
    <sonar.typescript.lcov.reportPaths>coverage/lcov.info</sonar.typescript.lcov.reportPaths>
    <d11-vertx-config.version>1.3.2</d11-vertx-config.version>
  </properties>
  <dependencies>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>com.dream11</groupId>
        <artifactId>config-maven-plugin</artifactId>
        <version>${d11-vertx-config.version}</version>
      </plugin>
      <plugin>
        <groupId>org.sonarsource.scanner.maven</groupId>
        <artifactId>sonar-maven-plugin</artifactId>
        <version>3.8.0.2131</version>
      </plugin>
    </plugins>
  </build>
</project>